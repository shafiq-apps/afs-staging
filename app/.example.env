# Development Environment Configuration
NODE_ENV=production

# Server Configuration
PORT=3554


# server credentials
ELASTICSEARCH_HOST=XXXXXXXXXXXXXXXXXXXXXXXXXXXXXX
ELASTICSEARCH_USERNAME=XXXXXXXXXXXXXXXXXXXXXXXXXX
ELASTICSEARCH_PASSWORD=XXXXXXXXXXXXXXXXXXXXXXXXXX

ELASTICSEARCH_CA_CERT_PATH=XXXXXXXXXXXXXXXXXXXXXX
ELASTICSEARCH_REJECT_UNAUTHORIZED=false
ELASTICSEARCH_PING_TIMEOUT=5000
ELASTICSEARCH_REQUEST_TIMEOUT=30000
ELASTICSEARCH_MAX_RETRIES=3

# Shopify Configuration
SHOPIFY_API_KEY=XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX
SHOPIFY_API_SECRET_KEY=XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX
SHOPIFY_API_VERSION=2026-01
SHOPIFY_API_MAX_RETRIES=4
SHOPIFY_API_BACKOFF_MS=500
SHOPIFY_API_MAX_WAIT_MS=10000

# App Credentials
APP_BACKEND_SECRET=XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX
CSRF_SECRET=XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX

# Cache TTL (milliseconds)
CACHE_ENABLED=false
# (or CACHE_DEFAULT_TTL) — default TTL for the underlying CacheManager entries
CACHE_TTL=600000
#TTL for search results cache
CACHE_SEARCH_TTL=600000
#TTL for filter (facet) cache
CACHE_FILTER_TTL=600000
#TTL for filter-config list cache
CACHE_FILTER_LIST_TTL=600000
#max entries per cache manager
CACHE_MAX_SIZE=2000
#cleanup interval for expired entries
CACHE_CHECK_INTERVAL=60000
#true|false — enable/disable cache hit/miss stats tracking
CACHE_STATS_ENABLED=true

# Indexing Configuration
INDEXER_BATCH_SIZE=5000
INDEXER_MAX_CONCURRENT_BATCHES=3
INDEXER_CHECKPOINT_DEBOUNCE_MS=2000

# Static File Serving (Optional - for development)
# STATIC_PUBLIC_PATH=public
# STATIC_REACT_BUILD_PATH=client/build
# STATIC_PREFIX=/static
# REACT_PREFIX=/
# ENABLE_SPA_FALLBACK=true
# STATIC_MAX_AGE=86400000

#true|false — disables all logs
LOG_ENABLED=true
#info|warn|error — minimum level to output
LOG_LEVEL=info
#comma-separated module names (e.g. cache,cache-service,storefront-repository)
#
LOG_DISABLED_MODULES=
#comma-separated allow-list (only these modules log)
LOG_ENABLED_MODULES=
# true|false — disables cache logs
CACHE_LOG_DISABLED=false

INDEXER_CHECKPOINTS_ENABLED=true

SHOPIFY_APP_HANDLE=filterstagingapp
SHOPIFY_APP_SUBSCRIPTIONS_TEST_MODE=true
SHOPIFY_APP_SUBSCRIPTIONS_TRIAL_DAYS=21

# App's Internal Security
API_KEY=XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX
API_SECRET=XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX

SENDGRID_API_KEY='XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX'
APP_EMAIL_FROM=support@xxxxxxxxxxxxxxxxxx.com
APP_EMAIL_NAME=XXXXXXXXXXXXXXXXXXXXX
SUPPORT_TICKET_EMAIL_TO='xxxxxxxxx@xxxxxxxxxx.com'
